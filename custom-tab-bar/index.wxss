.tabbar_box{
  background-color: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: absolute;
  bottom: 0;
  left: 0;
  /* z-index: 999; */
  z-index: 99;
  width: 100%;
  height: 95rpx;
  box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
}
.test{
  z-index: 10;
}
.tabbar_nav{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  height: 100%;
  position: relative;
}
.tabbar_icon{
  width: 60rpx;
  height: 60rpx;
}
.special-wrapper{
position: absolute;
top: -36rpx;
width: 96rpx;
height: 96rpx;
text-align: center;
line-height: 96rpx;
border-radius: 50%;
border-top: 2rpx solid #f2f2f3;
background-color: #fff;
}
.special-wrapper .tabbar_icon{
  width: 94rpx;
  height: 94rpx;
}
.special-text-wrapper{
  position: absolute;
  bottom: 10rpx;
}